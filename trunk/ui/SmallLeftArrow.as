package ui {		import flash.display.MovieClip;	import flash.events.MouseEvent;			public class SmallLeftArrow extends MovieClip 	{		public function SmallLeftArrow() 		{			addEventListener(MouseEvent.MOUSE_OUT, mouseOut);			addEventListener(MouseEvent.MOUSE_OVER, mouseOver);		}				private function mouseOut(e:MouseEvent) : void		{			this.gotoAndStop("Rest");		}				private function mouseOver(e:MouseEvent) : void		{			this.gotoAndStop("MouseOver");		}			}	}