package ui{		import flash.display.MovieClip;	import fl.text.TLFTextField;	import game.entity.City;	import game.Kingdom;	import game.KingdomManager;		public class FinancesUI extends Panel	{						public static var FINANCES_PANEL:int = 2;			public var incomeText:TLFTextField;		public var treasuryText:TLFTextField			public var city:City;				public var cityUI:CityUI;					public function FinancesUI() 		{			// constructor code		}				override public function hidePanel() : void		{			this.visible = false;		}				override public function showPanel() : void		{			this.visible = true;						setText();		}						private function setText() : void		{			var kingdom:Kingdom = KingdomManager.INSTANCE.getKingdom(city.playerId)						incomeText.text = UtilUI.FormatNum(city.getIncome());			treasuryText.text = UtilUI.FormatNum(kingdom.gold)		}	}	}