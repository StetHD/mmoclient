package ui {		import flash.display.MovieClip;	import fl.text.TLFTextField;	import flash.events.MouseEvent;		public class Pagination extends MovieClip 	{		public var currentPage:TLFTextField;		public var totalPages:TLFTextField;		public var leftArrow:MovieClip;		public var rightArrow:MovieClip;				public function Pagination() 		{			leftArrow.addEventListener(MouseEvent.MOUSE_OUT, mouseOut);			leftArrow.addEventListener(MouseEvent.MOUSE_OVER, mouseOver);			leftArrow.addEventListener(MouseEvent.MOUSE_DOWN, mouseDown);								rightArrow.addEventListener(MouseEvent.MOUSE_OUT, mouseOut);			rightArrow.addEventListener(MouseEvent.MOUSE_OVER, mouseOver);			rightArrow.addEventListener(MouseEvent.MOUSE_DOWN, mouseDown);										}				private function mouseOut(e:MouseEvent) : void		{			e.target.gotoAndStop("Rest");		}				private function mouseOver(e:MouseEvent) : void		{			e.target.gotoAndStop("MouseOver");		}				private function mouseDown(e:MouseEvent): void		{			e.target.gotoAndStop("MouseDown");		}		}	}