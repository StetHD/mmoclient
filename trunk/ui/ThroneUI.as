package ui {		import flash.display.MovieClip;		import fl.text.TLFTextField;		import game.entity.City;		import game.entity.Entity;		public class ThroneUI extends Panel	{						public static var THRONE_PANEL:int = 0;				public var contractsText:TLFTextField;		public var buildingsText:TLFTextField;		public var improvementsText:TLFTextField;			public var city:City;		public var cityUI:CityUI;				public function ThroneUI() 		{		}				override public function hidePanel() : void		{			this.visible = false;		}				override public function showPanel() : void		{			this.visible = true;						setText();		}						private function setText() : void		{			var inUseContracts:int = city.contracts.length;			var maxContracts:int = 99;			var inUseBuildings:int = city.getNumAssignedByEntity(Entity.BUILDING);			var numBuildings:int = city.buildings.length;			var inUseImprovements:int = city.getNumAssignedByEntity(Entity.IMPROVEMENT);			var numImprovements:int = city.improvements.length;						contractsText.text = inUseContracts + " " + maxContracts + " contracts in use";			buildingsText.text = numBuildings + " Buildings, " + inUseBuildings + " Active";			improvementsText.text = numImprovements + " Improvement, " + inUseImprovements			+ " Active";		}	}	}