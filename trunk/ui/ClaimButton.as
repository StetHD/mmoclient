package ui {		import flash.display.MovieClip;	import fl.text.TLFTextField;	import flash.events.MouseEvent;			public class ClaimButton extends MovieClip 	{		public var buttonName:TLFTextField;				public function ClaimButton() 		{			addEventListener(MouseEvent.MOUSE_OUT, mouseOut);			addEventListener(MouseEvent.MOUSE_OVER, mouseOver);		}				private function mouseOut(e:MouseEvent) : void		{			this.gotoAndStop(1);		}				private function mouseOver(e:MouseEvent) : void		{			this.gotoAndStop(2);		}			}	}