package ui {		import flash.display.MovieClip;	import fl.text.TLFTextField;	import flash.events.MouseEvent;			public class TrainContract extends MovieClip 	{		public var cityName:TLFTextField;		public var selectSource:MovieClip;		public var createNew:MovieClip;				public var closeButton:CloseButton;						public function TrainContract()		{			closeButton.addEventListener(MouseEvent.CLICK, closeButtonClick);				}				public function showPanel() : void		{			createNew.visible = false;								}				private function closeButtonClick(e:MouseEvent) : void		{			this.visible = false;		}	}	}