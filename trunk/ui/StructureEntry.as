package ui {		import flash.display.MovieClip;	import fl.text.TLFTextField;	import flash.display.BitmapData;	import flash.display.Bitmap;		public class StructureEntry extends MovieClip 	{		public var nameText:TLFTextField;		public var levelText:TLFTextField;		public var hpText:TLFTextField;				public function StructureEntry() 		{		}				public function setImage(bitmapData:BitmapData) : void		{					var image:Bitmap = new Bitmap(bitmapData);			image.x = 1;			image.y = 1;						addChild(image);					}			}	}