package game {	import flash.display.DisplayObject;	import net.packet.TransferItem;	public class LastTransferItem	{		public var itemId:int;		public var sourceId:int;		public var sourceType:int; 		public var sourceUI:DisplayObject;		public var targetId:int;		public var targetType:int;		public var targetUI:DisplayObject;		public function LastTransferItem() 		{			// constructor code		}		public function setTransferItem(transferItem:TransferItem, _sourceUI:DisplayObject, _targetUI:DisplayObject)		{			itemId = transferItem.itemId;			sourceId = transferItem.sourceId;			sourceType = transferItem.sourceType;			sourceUI = _sourceUI;			targetId = transferItem.targetId;			targetType = transferItem.targetType;			targetUI = _targetUI;		}	}	}