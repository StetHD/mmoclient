package game {	import game.entity.Entity;	import game.entity.EntityManager;	import game.entity.City;	import flash.utils.Dictionary;	import game.perception.PerceptionManager;		public class Claim 	{		public static var STATE_CLAIM:int = 12		public static var STATE_COMPLETED:int = 6;				public var id:int;		public var tileIndex:int;		public var cityId:int;		public var state:int;		public function Claim() 		{					}				public static function getClaimFromPos(xCoord:int, yCoord:int) : Claim		{			var entities:Dictionary = PerceptionManager.INSTANCE.getEntities();			var claim:Claim;									for (var entityId:Object in entities)			{						var entity:Entity = entities[entityId];								if(entity.type == Entity.CITY)								{					var city:City = City(entity);					claim = city.getClaimFromPos(xCoord, yCoord);										if(claim != null)					{						return claim;					}				}			}						return null;		}	}	}