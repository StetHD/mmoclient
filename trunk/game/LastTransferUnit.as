package game {	import flash.display.DisplayObject;	import net.packet.TransferUnit;	public class LastTransferUnit 	{		public var unitId:int;		public var sourceId:int;		public var sourceType:int; 		public var sourceUI:DisplayObject;		public var targetId:int;		public var targetType:int;		public var targetUI:DisplayObject;		public function LastTransferUnit() 		{			// constructor code		}		public function setTransferUnit(transferUnit:TransferUnit, _sourceUI:DisplayObject, _targetUI:DisplayObject)		{			unitId = transferUnit.unitId;			sourceId = transferUnit.sourceId;			sourceType = transferUnit.sourceType;			sourceUI = _sourceUI;			targetId = transferUnit.targetId;			targetType = transferUnit.targetType;			targetUI = _targetUI;		}	}	}