package stats {		public class ImprovementStat	{				public static var FARM_CATEGORY_NAME:String = "farm";		public static var TRAPPER_CATEGORY_NAME:String = "trapper";		public static var LUMBERMILL_CATEGORY_NAME:String = "lumbermill";		public static var MINE_CATEGORY_NAME:String = "mine";		public static var QUARRY_CATEGORY_NAME:String = "quarry";				public static var FARM_CATEGORY:int = 1;		public static var TRAPPER_CATEGORY:int = 2;		public static var LUMBERMILL_CATEGORY:int = 3;		public static var MINE_CATEGORY:int = 4;		public static var QUARRY_CATEGORY:int = 5;				public var id:int;		public var category:int;		public var level:int;		public var name:String;		public var totalHp:int;		public var populationCap:int;		public var productionCost:int;		public var goldCost:int;		public var lumberCost:int;		public var stoneCost:int;		public var upkeep:int;		public var image48:int;		public function ImprovementStat() {			// constructor code		}				public static function getCategoryId(category:String) : int		{			switch(category.toLowerCase())			{				case FARM_CATEGORY_NAME:					return FARM_CATEGORY;				case TRAPPER_CATEGORY_NAME:					return TRAPPER_CATEGORY;				case LUMBERMILL_CATEGORY_NAME:					return LUMBERMILL_CATEGORY;				case QUARRY_CATEGORY_NAME:					return QUARRY_CATEGORY;				case MINE_CATEGORY_NAME:					return MINE_CATEGORY;			}						return -1;		}	}	}